<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    
	<!-- ====== 定义实体类的别名 ====== -->
	<typeAliases>
	      
	    <!-- ==后台管理系统== -->
	    
	      <!-- 树节点-->
	      <typeAlias type="com.cjhme.system.main.bean.TreeNodeBean" alias="treeNodeBean"/>
	      
	      <!-- 菜单 -->
	      <typeAlias type="com.cjhme.system.menu.bean.MenuBean" alias="menuBean"/>
	      
	      <!-- 用户 -->
	      <typeAlias type="com.cjhme.system.user.bean.UserBean" alias="userBean"/>
	      
	      <!-- 角色 -->
	      <typeAlias type="com.cjhme.system.role.bean.RoleBean" alias="roleBean"/>
	      
	      <!-- 用户角色映射 -->
	      <typeAlias type="com.cjhme.system.user.bean.UserRoleMappingBean" alias="userRoleMappingBean"/>
	      
	      <!-- 部门 -->
	      <typeAlias type="com.cjhme.system.dept.bean.DeptBean" alias="deptBean"/>
	      
	      <!-- 用户部门映射 -->
	      <typeAlias type="com.cjhme.system.user.bean.UserDeptMappingBean" alias="userDeptMappingBean"/>
	      
	      <!-- 数据字典分类 -->
	      <typeAlias type="com.cjhme.system.datadic.bean.DataDicTypeBean" alias="dataDicTypeBean"/>
	      
	      <!-- 数据字典明细 -->
	      <typeAlias type="com.cjhme.system.datadic.bean.DataDicItemBean" alias="dataDicItemBean"/>
	      
	      <!-- 角色菜单映射 -->
	      <typeAlias type="com.cjhme.system.role.bean.RoleMenuMappingBean" alias="roleMenuMappingBean"/>
	      
	      <!-- 角色按钮映射 -->
	      <typeAlias type="com.cjhme.system.role.bean.RoleButtonMappingBean" alias="roleButtonMappingBean"/>
	      
	      <!-- 按钮 -->
	      <typeAlias type="com.cjhme.system.button.bean.ButtonBean" alias="buttonBean"/>
	      
	
	      <!-- 待办 -->
	      <typeAlias type="com.cjhme.system.task.bean.TaskBean" alias="taskBean"/>
	      
	      <!-- 登录日志 -->
	      <typeAlias type="com.cjhme.system.loginlog.bean.LoginLogBean" alias="loginLogBean"/>
	      
	      <!-- 文件数据 -->
	      <typeAlias type="com.cjhme.system.fileupload.bean.FileDataBean" alias="fileDataBean"/>
	      
	      <!-- 公告 -->
	      <typeAlias type="com.cjhme.system.notice.bean.NoticeBean" alias="noticeBean"/>
	      
	      <!-- 公告用户映射-->
	      <typeAlias type="com.cjhme.system.notice.bean.NoticeUserMappingBean" alias="noticeUserMappingBean"/>
	      
	      <!-- 数据字典明细角色映射-->
	      <typeAlias type="com.cjhme.system.datadic.bean.DataDicItemRoleMappingBean" alias="dataDicItemRoleMappingBean"/>
	      
	      <!-- 访问地址-->
	      <typeAlias type="com.cjhme.system.access.bean.AccessUrlBean" alias="accessUrlBean"/>
	      
	      <!-- 角色访问地址映射-->
	      <typeAlias type="com.cjhme.system.role.bean.RoleAccessUrlMappingBean" alias="roleAccessUrlMappingBean"/>
	      
	      <!-- 常用功能-->
	      <typeAlias type="com.cjhme.system.comfunc.bean.ComFuncBean" alias="comFuncBean"/>
	      
	      
	      
	    <!-- ==前台页面== -->  
	      
	</typeAliases>


	<!-- ===定义映射资源=== -->
	<mappers>
	    <!-- ==后台管理系统== -->
	    
	      <!-- 菜单 -->
	      <mapper resource="com/cjhme/system/menu/mapper/MenuDao.xml"/>
	      
	      <!-- 用户 -->
	      <mapper resource="com/cjhme/system/user/mapper/UserDao.xml"/>
	      
	      <!-- 角色 -->
	      <mapper resource="com/cjhme/system/role/mapper/RoleDao.xml"/>
	      
	      <!-- 数据字典 -->
	      <mapper resource="com/cjhme/system/datadic/mapper/DataDicDao.xml"/>
	      
	      <!-- 部门 -->
	      <mapper resource="com/cjhme/system/dept/mapper/DeptDao.xml"/>
	      
	      <!-- 按钮 -->
	      <mapper resource="com/cjhme/system/button/mapper/ButtonDao.xml"/>
	      
	      <!-- 待办 -->
	      <mapper resource="com/cjhme/system/task/mapper/TaskDao.xml"/>
	      
	      <!-- 登录日志 -->
	      <mapper resource="com/cjhme/system/loginlog/mapper/LoginLogDao.xml"/>
	      
	      <!-- 文件数据 -->
	      <mapper resource="com/cjhme/system/fileupload/mapper/FileDataDao.xml"/>
	      
	      <!-- 公告 -->
	      <mapper resource="com/cjhme/system/notice/mapper/NoticeDao.xml"/>
	      
	      <!-- 访问地址 -->
	      <mapper resource="com/cjhme/system/access/mapper/AccessUrlDao.xml"/>
	      
	      <!-- 常用功能 -->
	      <mapper resource="com/cjhme/system/comfunc/mapper/ComFuncDao.xml"/>
	      
	      
	      <!-- demo -->
	      <mapper resource="com/cjhme/demo/mapper/MasterDemoDao.xml"/>
	      <mapper resource="com/cjhme/demo/mapper/OtherDemoDao.xml"/>
	      
	     
	    <!-- ==前台页面== -->
	     
	</mappers>

</configuration>  